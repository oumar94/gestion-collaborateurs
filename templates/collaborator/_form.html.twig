{{ form_start(form) }}

<div class="row">
    <div class="col-md-4">{{ form_row(form.nom) }}</div>
    <div class="col-md-5">{{ form_row(form.prenom) }}</div>

</div>
<div class="row">
    <div class="col-md-4">{{ form_row(form.date_dispo) }}</div>
    <div class="col-md-4">{{ form_row(form.date_depart_prevue) }}</div>
</div>
<div class="row">
    <div class="col-md-2">{{ form_row(form.imageFile) }}</div>
</div>
{{ form_rest(form) }}
